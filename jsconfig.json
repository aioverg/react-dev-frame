{
  /**
    处理alias转跳问题
    https://code.visualstudio.com/docs/languages/jsconfig
  **/
  "compilerOptions": {
    "checkJs": false,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "@src/*": [
        "src/*"
      ],
      "@components": [
        "src/components"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@utils": [
        "src/utils"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@router/*": [
        "src/router/*"
      ],
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}